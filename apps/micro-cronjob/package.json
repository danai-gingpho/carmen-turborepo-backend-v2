{
  "name": "micro-cronjob",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",   
  "exports": {
    ".": "./dist/index.js",
    "./dist": "./dist/index.js",
    "./dist/*": "./dist/*",
    "./generated": "./generated/index.ts",
    "./generated/*": "./generated/*"
  },
  "scripts": {
    "build": "bun build src/server.ts --outdir=dist --target=bun",
    "dev": "bun run --watch src/server.ts",
    "dev:cronjob": "bun run --watch src/server.ts",
    "dev:prod": "bun run src/server.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "bun dist/server.js",
    "start:dev": "bun dist/server.js",
    "start:debug": "bun dist/server.js --watch",
    "start:prod": "bun dist/server.js",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "postinstall": "bun run prisma generate",
    "db:generate": "bunx prisma generate",
    "db:push": "bunx prisma db push",
    "db:studio": "bunx prisma studio",
    "db:seed": "bun run prisma/seed.ts",
    "db:migrate": "prisma migrate dev --skip-generate",
    "db:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:seed:reset": "prisma migrate reset --force && ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/openapi": "^1.4.11",
    "@elysiajs/swagger": "^1.3.1",
    "@prisma/client": "^6.16.2",
    "@sinclair/typebox": "^0.34.41",
    "artillery": "^2.0.26",
    "elysia": "^1.4.9",
    "node-cron": "^4.2.1"
  },
  "devDependencies": {
    "@types/node-cron": "^3.0.11",
    "bun-types": "latest",
    "prisma": "^6.16.2",
    "prisma-types-generator": "^1.3.2"
  },
  "module": "src/index.js"
}
